<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Participacion.
    -->
    <changeSet id="20161010163649-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="actividad_id"
                                 baseTableName="participacion"
                                 constraintName="fk_participacion_actividad_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actividad"/>

        <addForeignKeyConstraint baseColumnNames="empleado_id"
                                 baseTableName="participacion"
                                 constraintName="fk_participacion_empleado_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empleado"/>

    </changeSet>
</databaseChangeLog>
